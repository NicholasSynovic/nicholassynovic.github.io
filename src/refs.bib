	keywords = {author profiling, authorship attribution, authorship verification, source code analysis},
	pages = {131},
	file = {Full Text PDF:/home/nicholas/Zotero/storage/6QRCD2W6/He et al. - 2024 - Authorship Attribution Methods, Challenges, and Future Research Directions A Comprehensive Survey.pdf:application/pdf},
